{% extends '::base.html.twig' %}
{% block body %}
<h1>Listado de Articulos</h1>
<table border=”1”>
<tr>
<th>ID</th>
<th>Titulo</th>
<th>Descripcion</th>
</tr>
{% for articulo in articulos %}
<tr>
<td>{{ articulo.id }}</td>
<td>{{ articulo.titulo }}</td>
<td>{{ articulo.descripcion}}</td>
</tr>
{% endfor %}
</table>
{% endblock %}